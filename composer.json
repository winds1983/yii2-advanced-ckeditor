{
    "name": "winds1983/yii2-ckeditor",
    "description": "Yii 2 CKEditor",
    "keywords": [
        "yii2",
        "extension",
        "widget",
        "ckeditor"
    ],
    "homepage": "https://github.com/winds1983/yii2-ckeditor",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Sun",
            "email": "dev.winds@gmail.com",
            "homepage": "http://www.hivesoftware.com/"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "hive\\ckeditor\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}